{
  "name": "last-found-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "schema": "./src/database/prisma/schema.prisma",
    "seed": "ts-node --transpile-only ./src/database/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:restart": "npx prisma db push && npx prisma generate && nodemon src/index.ts",
    "build": "tsc --project .",
    "start": "npx ts-node src/index.ts",
    "restart": "npx prisma db push && npx prisma generate && yarn start",
    "db:seed": "ts-node --transpile-only ./src/database/prisma/seed.ts",
    "linux:kill": "fuser -k 3030/tcp",
    "test": "npx ts-node test.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.19",
    "typescript": "^4.8.2"
  }
}
